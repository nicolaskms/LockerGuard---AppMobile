<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    tools:context=".GerenteQrCode"
    android:background="#d2d0d3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">>

<fragment
    android:layout_width="match_parent"
    android:layout_height="700dp"
    tools:ignore="FragmentTagUsage"
    android:id="@+id/"
    tools:context="."
    android:name="com.google.android.gms.t" />
<TextView
    android:id="@+id/TextViewMaps"
    android:textAllCaps="false"
    android:layout_width="200dp"
    android:text="@string/"
    android:textSize="15dp"
    android:textAlignment="center"
    android:textColor="@color/"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginTop="16dp"/>
<Button
    android:id="@+id/"
    android:textAllCaps="false"
    android:layout_width="130dp"
    android:hint="@string/btn_cadastrar_card"
    android:textColorHint="@color/"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginTop="16dp"/>
<Button
    android:id="@+id/"
    android:textAllCaps="false"
    android:layout_width="130dp"
    android:hint="@string/btn_sair"
    android:textColorHint="@color/white"
    android:layout_height=""
    android:layout_gravity="center"
    android:layout_marginTop="16dp"/>


</androidx.appcompat.widget.LinearLayoutCompat>